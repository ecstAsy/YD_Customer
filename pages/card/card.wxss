@import "../../templete/card/card.wxss";
@import "../../templete/loading/loading.wxss";
@import "../../templete/info/info.wxss";
@import "../../templete/cardModal/modal.wxss";
@import "../../templete/phoneModal/modal.wxss";
page {
  background: #f5f5f5;
  font-family: "PingFang-SC-Regular";
  border-top: 1rpx solid #e2e4e8;
}
.getCard_box{
  width: 100%;
  height: 78rpx;
  padding: 19rpx 0 19rpx 0;
  position: fixed;
  background: #f5f5f5;
  z-index: 99;
  top: 0;
  left: 0;
}
.getCard {
  width: 100%;
  height: 78rpx;
  text-align: center;
  background: #fff;
}

.getCard image {
  width: 36rpx;
  height: 36rpx;
  vertical-align: middle;
  margin: 0 6rpx;
}

.getCard text {
  font-size: 29rpx;
  line-height: 78rpx;
  margin: 0 6rpx;
  color: #ff8b00;
  font-family: "PingFang-SC-Bold";
  font-weight: 600;
}

.card {
  padding-top: 97rpx;
}
.order_null{
  position: fixed;
  width: 100%;
  height: 50rpx;
  top: 30%;
  font-size: 28rpx;
  line-height: 50rpx;
  color: #ccc;
  text-align: center;
}